class Solution:
    def compress(self, chars: List[str]) -> int:
        write=0
        read=0
        while read < len(chars):
            current=chars[read]
            count=0
            while read <len(chars) and current==chars[read]:
                count+=1
                read+=1
            chars[write] = current
            write+=1
            if count>1:
                for digits in str(count):
                    chars[write] = digits
                    write +=1
        return write               
       
       
            
